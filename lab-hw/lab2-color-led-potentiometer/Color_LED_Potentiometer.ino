const int RED_POT_PIN = A0;const int GREEN_POT_PIN = A1;const int BLUE_POT_PIN = A2;const int LED_RED_PIN = 3;const int LED_GREEN_PIN = 5;const int LED_BLUE_PIN = 6;void setup() {  pinMode(LED_RED_PIN, OUTPUT);  pinMode(LED_GREEN_PIN, OUTPUT);  pinMode(LED_BLUE_PIN, OUTPUT);    Serial.begin(9600);}void loop() {  const int redPotValue = analogRead(RED_POT_PIN);  const int greenPotValue = analogRead(GREEN_POT_PIN);  const int bluePotValue = analogRead(BLUE_POT_PIN);    Serial.print("R: ");  Serial.print(redPotValue);  Serial.print(" G: ");  Serial.print(greenPotValue);  Serial.print(" B: ");  Serial.print(bluePotValue);  Serial.println();    analogWrite(LED_RED_PIN, map(redPotValue, 0, 1023, 0, 255));  analogWrite(LED_GREEN_PIN, map(greenPotValue, 0, 1023, 0, 255));  analogWrite(LED_BLUE_PIN, map(bluePotValue, 0, 1023, 0, 255));  delay(200);}