const int BUTTON_PIN = 2;const int LED_PIN = LED_BUILTIN;int buttonValue = 0int prevButtonValue = 0;int ledValue = 0;int counter = 0;void setup() {  pinMode(BUTTON_PIN, INPUT_PULLUP);  pinMode(LED_PIN, OUTPUT);  Serial.begin(9600);}void loop() {  buttonValue = digitalRead(BUTTON_PIN);  if (prevButtonValue != buttonValue) {    if (buttonValue == LOW) {      ++counter;      ledValue = !ledValue;    }    prevButtonValue = buttonValue;  }    Serial.println(counter);    digitalWrite(LED_PIN, ledValue);}